#include <bits/stdc++.h>
using namespace std;

int main()
{
    int counter = 1;

    long long x;
    cin >> x;

    long long num[x];

    for (int i = 0; i < x; ++i)
    {
        cin >> num[i];
    }
   
   cout << num[x] << "\n";

    
    long long n = sizeof(num) / sizeof(num[0]);
    
    for (int i = 0; i < n; ++i){

        if (num[i] % 2 == 0)
        {
            cout << "Case #" << counter++ << ":" << " " << "Even" << "\n";
        }
        else
        {
            cout << "Case #" << counter++ << ":" << " " << "Odd" << "\n";
        }
    }
    return 0;
}

